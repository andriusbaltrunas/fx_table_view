<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Pane prefHeight="624.0" prefWidth="660.0">
         <children>
            <TableView fx:id="tableView" layoutX="4.0" prefHeight="293.0" prefWidth="652.0">
              <columns>
                <TableColumn fx:id="id" prefWidth="155.0" text="ID" />
                <TableColumn fx:id="name" prefWidth="207.0" text="Name" />
                  <TableColumn fx:id="surname" prefWidth="289.0" text="Surname" />
              </columns>
            </TableView>
            <Button layoutX="155.0" layoutY="563.0" mnemonicParsing="false" onAction="#newItem" text="New Items" />
            <Button layoutX="259.0" layoutY="563.0" mnemonicParsing="false" onAction="#editItem" text="EDIT" />
            <ListView fx:id="itemsListView" layoutX="4.0" layoutY="303.0" onMouseClicked="#selectedListItem" prefHeight="200.0" prefWidth="652.0" />
         </children>
      </Pane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
