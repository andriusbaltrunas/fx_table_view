<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>


<GridPane alignment="center" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/null" fx:controller="sample.Controller">
   <children>
      <Pane prefHeight="624.0" prefWidth="660.0">
         <children>
            <TableView fx:id="tableView" layoutX="4.0" prefHeight="523.0" prefWidth="652.0">
              <columns>
                <TableColumn fx:id="id" prefWidth="155.0" text="ID" />
                <TableColumn fx:id="name" prefWidth="207.0" text="Name" />
                  <TableColumn fx:id="surname" prefWidth="289.0" text="Surname" />
              </columns>
            </TableView>
            <Button layoutX="155.0" layoutY="563.0" mnemonicParsing="false" onAction="#newItem" text="New Items" />
            <Button layoutX="259.0" layoutY="563.0" mnemonicParsing="false" onAction="#editItem" text="EDIT" />
         </children>
      </Pane>
   </children>
</GridPane>
